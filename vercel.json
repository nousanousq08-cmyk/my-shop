{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "dashboard/dashboard.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/dashboard", "dest": "/dashboard/dashboard.html" },
    { "src": "/dashboard/(.*)", "dest": "/dashboard/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
